{
  "name": "unlockr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "start": "react-native start",
    "setDevelopment": "ENVFILE=.env.dev",
    "setStaging": "ENVFILE=.env.uat",
    "setProduction": "ENVFILE=.env.prod",
    "ios:dev": "yarn setDevelopment react-native run-ios --mode=Debug --scheme \"dev\"",
    "ios:uat": "yarn setStaging  react-native run-ios --mode=Debug --scheme \"uat\"",
    "ios:prod": "yarn setProduction  react-native run-ios --mode=Debug --scheme \"prod\"",
    "android:dev": "yarn setDevelopment  react-native run-android --mode=developmentDebug --appIdSuffix=dev",
    "pod-install:dev": "yarn install && cd ios && bundle exec pod install",
    "pod-install:release": "yarn install && cd ios && NO_FLIPPER=1 bundle exec pod install",
    "clean:ios": "rm -rf node_modules yarn.lock && cd ios && rm -rf unlockr.xcworkspace Pods Podfile.lock .xcode.env.local",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint --ext .ts --ext .tsx ./src --fix",
    "prepare": "husky",
    "precommit": "lint-staged --no-stash",
    "commit": "git-cz",
    "no-commit-to-master-develop": "if [ \"$PUBLISH\" != true ] && ([ \"$(git symbolic-ref HEAD)\" = 'refs/heads/develop' ] || [ \"$(git symbolic-ref HEAD)\" = 'refs/heads/master' ]); then echo 'Direct commits/pushes to the branches develop or master are not allowed'; exit 1; fi",
    "test": "jest",
    "postinstall": "patch-package",
    "clean:android": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.10",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.14",
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/native-stack": "^6.9.25",
    "@react-navigation/stack": "^6.3.21",
    "@reduxjs/toolkit": "^2.2.0",
    "@shopify/restyle": "1.6.0",
    "axios": "^1.6.7",
    "date-fns-tz": "^3.1.3",
    "formik": "^2.4.5",
    "i18next": "^23.8.2",
    "jwt-decode": "^4.0.0",
    "react": "18.2.0",
    "react-i18next": "^14.0.5",
    "react-native": "0.74.1",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.12.0",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^8.1.2",
    "react-native-permissions": "^4.1.4",
    "react-native-reanimated": "3.12.0",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-size-matters": "^0.4.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.3.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-webview": "^13.7.1",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@tsconfig/react-native": "^3.0.0",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^29.5.12",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.2.18",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "git-cz": "^4.9.0",
    "husky": "^9.0.10",
    "jest": "^29.6.3",
    "lint-staged": "^13.2.3",
    "patch-package": "^8.0.0",
    "path-browserify": "^1.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.1.7",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn format",
      "yarn lint"
    ]
  }
}
